{
  "name": "spl-8004-sdk",
  "version": "1.0.0",
  "description": "Official TypeScript SDK for SPL-8004 protocol - decentralized AI agent payments on Solana",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "prepublishOnly": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "solana",
    "blockchain",
    "identity",
    "reputation",
    "ai-agents",
    "web3",
    "spl-8004",
    "decentralized",
    "crypto"
  ],
  "author": "Noema Protocol",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/NoemaProtocol/SPL-8004.git",
    "directory": "sdk"
  },
  "bugs": {
    "url": "https://github.com/NoemaProtocol/SPL-8004/issues"
  },
  "homepage": "https://noemaprotocol.xyz",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@solana/web3.js": "^1.87.0"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.30.1",
    "@solana/spl-token": "^0.4.0",
    "bs58": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
